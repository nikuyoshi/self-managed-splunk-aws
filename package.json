{
  "name": "self-managed-splunk-aws",
  "version": "0.1.0",
  "bin": {
    "self-managed-splunk-aws": "bin/self-managed-splunk-aws.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "deploy:interactive": "node scripts/interactive-deploy.js",
    "deploy:es": "npm run build && cdk deploy --all --context enableES=true --context enableLicense=true",
    "deploy:basic": "npm run build && cdk deploy --all",
    "deploy:production": "npm run build && cdk deploy --all --context enableES=true --context enableLicense=true --context indexerCount=6 --context indexerInstanceType=m7i.2xlarge"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "22.7.9",
    "aws-cdk": "2.171.0",
    "inquirer": "^8.2.6",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.3"
  },
  "dependencies": {
    "aws-cdk-lib": "2.171.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  }
}
